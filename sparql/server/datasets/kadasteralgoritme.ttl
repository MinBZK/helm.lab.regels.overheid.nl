        skos:narrower             ar:MachineLearningAlgorithm , ar:RuleBasedAlgorithm ;
        skos:prefLabel            "algoritme"@nl , "algorithm"@en ;
        skos:related              ar:ApplicationUrl , ar:AlgorithmApplication , ar:MPRD , ar:MethodsAndModels , ar:Publiccode , ar:SourceData ;
        skos:topConceptOf         schar:Algoritmeregister .

