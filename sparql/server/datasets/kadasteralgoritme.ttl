colar:AgorithmTypes  a   skos:Collection ;
        rdfs:label       "algorithm types"@en ;
        skos:definition  "The collection of possible algorithm types"@en , "De verzameling van mogelijke types van algoritmes"@nl ;
        skos:member      ar:MachineLearningAlgorithm , ar:RuleBasedAlgorithm ;
        skos:prefLabel   "algorithm types"@en , "types algoritme"@nl .

ar:MachineLearningAlgorithm
        a                        skos:Concept ;
        rdfs:label               "rule based"@en ;
        skosthes:broaderGeneric  ar:Algorithm ;
        skos:altLabel            "case based algorithm"@en , "self-learning algorithm"@en ;
        skos:broader             ar:Algorithm ;
        skos:definition          "A machine learning algorithm can discover connections in data themselves."@en , "Zelflerende algoritmes kunnen zelf verbanden ontdekken in gegevens."@nl ;
        skos:inScheme            schar:Algoritmeregister ;
        skos:prefLabel           "machine learning algorithm"@en , "zelflerend algoritme"@nl ;
        skos:scopeNote           "This involves an algorithm that has been developed based on examples that are offered to an AI application. Based on this learning material, a computer builds algorithms in a black box (machine learning). Deep learning is a form of machine learning in which this happens in several layers. A good example of this is an AI application for text analysis that can interpret ordinary Dutch in a first layer and legal jargon in a second layer."@en , "Hierbij gaat om een algoritme dat is ontwikkeld op basis van voorbeelden die worden aangeboden aan een AI-toepassing. Op basis van dit leermateriaal worden door een computer, in een black box, algoritmes opgebouwd (machine learning). Deep learning is een vorm van machine learning waarbij dit in meerdere lagen gebeurt. Een mooi voorbeeld hiervan is een AI-toepassing voor tekstanalyse die in een eerste laag gewoon Nederlands kan interpreteren en in een tweede laag juridisch jargon."@nl .

ar:RuleBasedAlgorithm
        a                        skos:Concept ;
        rdfs:label               "rule based algorithm"@en ;
        skosthes:broaderGeneric  ar:Algorithm ;
        skos:broader             ar:Algorithm ;
        skos:definition          "An algorithm that is run based on human-formulated rules and manually built by humans, usually based on laws or regulations."@en , "Een algoritme dat wordt uitgevoerd op basis van door mensen geformuleerde regels en zijn handmatig door mensen gebouwd, meestal op basis van wetten of verordeningen."@nl ;
        skos:inScheme            schar:Algoritmeregister ;
        skos:prefLabel           "rule based algorithm"@en , "regelgebaseerd algoritme"@nl .

ar:Area  a                         skos:Concept ;
        rdfs:label                 "geographical area"@en ;
        skosthes:broaderPartitive  ar:Metadata ;
        skos:broader               ar:Metadata ;
        skos:definition            "Official name of the geographical area in which the algorithm is being used"@en , "Het geografische gebied waarin het algoritme wordt ingezet."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "geografisch gebied"@nl , "geographical area"@en .

ar:Metadata  a                      skos:Concept ;
        rdfs:label                  "metadata"@en ;
        skosthes:narrowerPartitive  ar:Area , ar:ContactEmail , ar:Id , ar:Lang , ar:RevisionDate , ar:Schema , ar:Url ;
        skos:definition             "The metadata of an algorithm application."@en , "De metadata van een algoritmische toepassing."@nl ;
        skos:inScheme               schar:Algoritmeregister ;
        skos:narrower               ar:Area , ar:ContactEmail , ar:Id , ar:Lang , ar:RevisionDate , ar:Schema , ar:Url ;
        skos:prefLabel              "metadata"@nl , "metadata"@en ;
        skos:related                ar:AlgorithmApplication ;
        skos:topConceptOf           schar:Algoritmeregister .

schar:Algoritmeregister
        a                   skos:ConceptScheme ;
        rdfs:label          "concept scheme Dutch algorithm register"@en ;
        dct:description     "Een thesaurus van alle concepten in de registratie van de inzet van modellen, algoritmes en AI in beleid en besluitvormingsprocessen in Nederlandse 0verheidsorganisaties." ;
        dct:language        "nl" ;
        dct:modified        "2023-04-06T12:52:29+00:00"^^xsd:dateTime ;
        dct:rights          <https://creativecommons.org/licenses/by/4.0/deed.nl> ;
        skos:editorialNote  "Dit begrippenkader is nog niet formeel vastgesteld. De definities zijn ontleend aan de eerste versie van het algoritmeregister zoals gepubliceerd op  https://algoritmes.overheid.nl/. Dit is grotendeels gebaseerd op de versie 0.2 van https://standaard.algoritmeregister.org/."@nl ;
        skos:hasTopConcept  ar:Metadata , ar:BasicInformation , ar:Legal , ar:UseCase , ar:Oversight , ar:Application , ar:Algorithm , ar:AlgorithmApplication , ar:AlgorithmRegister ;
        skos:prefLabel      "Concept scheme Dutch algorithm register"@en , "Begrippenkader algoritmeregister"@nl .

ar:Category  a                     skos:Concept ;
        rdfs:label                 "category"@nl ;
        skosthes:broaderPartitive  ar:BasicInformation ;
        skos:altLabel              "theme"@en , "thema"@nl , "domain"@en , "domein"@nl ;
        skos:broader               ar:BasicInformation ;
        skos:definition            "The domain in which the algorithm will be deployed?"@en , "Het domein waarin het algoritme wordt ingezet."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "categorie"@nl , "category"@en ;
        skos:scopeNote             "possible values: taxes; living, working and living environment; culture; sports and leisure; defense; economics and entrepreneurship, family, youth and households; finance; general; health and health care; immigration and integration; international cooperation and development aid; agriculture, nature and food; environment, spatial planning and water; education and science; public order and safety; government; justice; travel, live and work abroad; subsidies and allowances; Traffic and transport; work and career"@dn , "mogelijke waarden: belastingen; wonen, werken en leefomgeving; cultuur; sport en vrije tijd; defentie; economie en ondernemerschap, gezin, jeugd en huishoudens; financiën; algemeen; gezondheid en gezondheidszorg; immigratie en integratie; internationale samenwerking en ontwikkelingshulp; landbouw, natuur en voedsel; milieu, ruimtelijke ordening en water; onderwijs en wetenschap; openbare orde en veiligheid; overheid; justitie; reizen, wonen en werken in het buitenland; subsidies en toeslagen; verkeer en vervoer; werk en carrière"@nl .

ar:BasicInformation  a              skos:Concept ;
