        skos:inScheme               schar:Algoritmeregister ;
        skos:narrower               ar:CompetentAuthority , ar:DPIA , ar:DPIADescription , ar:LawfulBasis , ar:ObjectionProcedure ;
        skos:prefLabel              "legal"@en , "juridisch"@nl ;
        skos:related                ar:AlgorithmApplication ;
        skos:topConceptOf           schar:Algoritmeregister .

ar:ContactEmail  a                 skos:Concept ;
        rdfs:label                 "contact person e-mail"@en ;
        skosthes:broaderPartitive  ar:Metadata ;
        skos:broader               ar:Metadata ;
        skos:definition            "E-mail address of the contact person for this registration"@en , "e-mailadres van de organisatie of de contactpersoon voor deze registratie."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "contact person e-mail"@en , "E-mailadres van de contactpersoon"@nl .

ar:DPIA  a                         skos:Concept ;
        rdfs:label                 "Data protection impact assessment"@en ;
        skosthes:broaderPartitive  ar:Legal ;
        skos:broader               ar:Legal ;
        skos:definition            "Has a data protection impact assessment been carried out?"@en , "Is er een data protection impact assessment (DPIA) uitgevoerd?"@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "data protection impact assessment"@nl , "data protection impact assessment"@en .

ar:DPIADescription  a              skos:Concept ;
        rdfs:label                 "description of the DPIA"@en ;
        skosthes:broaderPartitive  ar:Legal ;
        skos:broader               ar:Legal ;
        skos:definition            "An overview of the key points from the data protection impact assessment. It should explain how discrimination is prevented when (proxies of) ethnicity, sex or zipcode are being used."@en , "Een overzicht van de belangrijkste zaken die uit de data protection impact assessment (DPIA) naar voren kwamen."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "description of the DPIA"@en , "omschrijving van de DPIA"@nl .

ar:DecisionMakingProcess
        a                          skos:Concept ;
        rdfs:label                 "decision-making process"@en ;
        skosthes:broaderPartitive  ar:UseCase ;
        skos:broader               ar:UseCase ;
        skos:definition            "The formal process in the organisation in which the algorithm will be/is involved?"@en , "De naam van het bedrijfs- of besluitvormingsproces in de organisatie waarbinnen het algoritme wordt ingezet."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "besluitvormingsproces"@nl , "decision-making process"@en .

