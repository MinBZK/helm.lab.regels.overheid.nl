        rdfs:label                 "description"@en ;
        skosthes:broaderPartitive  ar:Application ;
        skos:broader               ar:Application ;
        skos:definition            "An extensive description between 500 and 10000 chars explaining the inner workings of the algorithm. It should detail all relevant aspects that are needed to understand how the algorithm processes data and feeds decision making."@en , "Een uitgebreide uitleg tussen de 500 en 10000 karakters van hoe het  algoritme werkt."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "description"@en , "omschrijving"@nl ;
        skos:related               ar:AlgorithmApplication .

ar:AlgorithmApplication
        a                  skos:Concept ;
        rdfs:label         "algorithm application"@en ;
        skos:definition    "An application that uses algorithms."@en , "Een toepassing waarbij algoritmen worden ingezet."@nl ;
        skos:inScheme      schar:Algoritmeregister ;
        skos:prefLabel     "algorithm application"@en , "algoritmische toepassing"@nl ;
        skos:related       ar:Metadata , ar:BasicInformation , ar:Legal , ar:UseCase , ar:Oversight , ar:Algorithm , ar:Description , ar:AlgorithmRegister ;
