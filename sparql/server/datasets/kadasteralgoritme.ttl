        skosthes:narrowerPartitive  ar:Category , ar:Organization , ar:DescriptionShort , ar:Name , ar:Status , ar:Type , ar:Website ;
        skos:definition             "The basic information on the algorithm application."@en , "De algemene informatie over de algoritmische toepassing."@nl ;
        skos:inScheme               schar:Algoritmeregister ;
        skos:narrower               ar:Category , ar:Organization , ar:DescriptionShort , ar:Name , ar:Status , ar:Type , ar:Website ;
        skos:prefLabel              "basic information"@en , "algemene informatie"@nl ;
        skos:related                ar:AlgorithmApplication ;
        skos:topConceptOf           schar:Algoritmeregister .

