        skos:definition            "Please indicate which known methods or models the algorithm is using."@en , "Standaard methoden of modellen die het algoritme gebruikt."@nl ;
        skos:editorialNote         "An expandable list of methods and models would be usefull."@en , "Een uitbreidbare lijst met methoden en modellen zou helpen."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "methods and models"@en , "methoden en modellen"@nl ;
