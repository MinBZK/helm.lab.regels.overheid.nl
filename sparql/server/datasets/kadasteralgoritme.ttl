        skos:definition             "The way in which the algorithm application is used."@en , "De wijze waarop de algoritmische toepassing wordt ingezet."@nl ;
        skos:inScheme               schar:Algoritmeregister ;
        skos:narrower               ar:DecisionMakingProcess , ar:Documentation , ar:Goal , ar:Impact , ar:Proportionality ;
        skos:prefLabel              "use case"@en , "inzet"@nl ;
        skos:related                ar:AlgorithmApplication ;
        skos:topConceptOf           schar:Algoritmeregister .

ar:Department  a                   skos:Concept ;
        rdfs:label                 "department"@en ;
        skosthes:broaderPartitive  ar:Organization ;
        skos:altLabel              "divisie"@nl , "division"@en ;
        skos:broader               ar:Organization ;
        skos:definition            "The full name of the department or division used to specify responsibility for use of the model, algorithm or AI."@en , "De volledige naam van de afdeling of divisie verantwoordelijk voor de inzet van het algoritme."@nl ;
        skos:inScheme              schar:Algoritmeregister ;
        skos:prefLabel             "afdeling"@nl , "department"@en .

ar:Organization  a                  skos:Concept ;
        rdfs:label                  "organization"@en ;
